    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "prompts": "^2.3.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.8.3"
  },
  "lint-staged": {
    "src/**/.ts": [
      "ng lint --fix"
